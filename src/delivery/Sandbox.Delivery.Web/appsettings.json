{
  "ConnectionStrings": {
    "CMSConnectionString": "Data Source=localhost\\sqlexpress;Initial Catalog=ken13-beta-3;Integrated Security=True;Persist Security Info=False;Connect Timeout=60;Encrypt=False;Current Language=English;"
  },
  "CMSHashStringSalt": "#{ CMSHashStringSalt }#",
  "CMSAzureAccountName": "#{ CMSAzureAccountName }#",
  "CMSAzureSharedKey": "#{ CMSAzureSharedKey }#",
  "CMSAzureBlobEndPoint": "#{ CMSAzureBlobEndPoint }#",

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AllowedHosts": "*",

  "azure-storage": {
    "is-enabled": true,
    "container-name": "#{ azure-storage:container-name }#"
  },

  "cms": {
    "admin-url": "http://localhost:44372/"
  },

  "cache": {
    "query-result": {
      "duration-seconds": 300
    },
    "sitemap-result": {
      "duration-minutes": 5
    },
    "response-cache": {
      "is-enabled": true
    }
  }
}